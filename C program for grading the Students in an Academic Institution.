#include<stdio.h>


int main (){

int num;
printf("Enter The Number: \n");
scanf("%d",&num);
switch(num/10){
	
	case 10:printf("Grade = O");
           break;
    
	case 9:printf("Grade = E");
           break;
           
	case 8:printf("Grade = A");
           break;
		   
	case 7:printf("Grade = B");
           break;
           
	case 6:printf("Grade = C");
           break;
           
    case 5:printf("Grade = D");
           break;
           
	default : printf("F");  	          
             break;
}	
	
	return 0;
}
