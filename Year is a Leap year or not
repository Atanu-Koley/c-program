#include<stdio.h>
int main(){
	
	int year ;
	printf(" year is = ");
	scanf("%d",&year);
	
	if(year%4==0){
		printf("Year is a Leap year ");
	}
	else if(year%400==0){
		printf("Year is a Leap year ");
	}
	else if(year%100==0){
		printf("Year is a Leap year ");
	}
	else{
		printf("Year not a Leap year ");
		
	}
	return 0;
}
