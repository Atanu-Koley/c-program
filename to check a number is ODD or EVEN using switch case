#include<stdio.h>


int main(){
	
	int num;
	printf("Enter number : \n");
	scanf("%d",&num);
	
	switch(num%2){
		
		case 1:printf("Number is ODD");
		       break;
		       
	    case 0:printf("Number is EVEN");
		       break;
		             
		default : printf("Wrong num");	
		        break;	
	}
	
	return 0;
}
